<rerunConfig>
	<jenkins>
		<!-- must must-->
		<jenkinsFolder>C:\Program Files (x86)\Jenkins</jenkinsFolder>
		<!-- optional: default is all view-->
		<views>
			<!--<listView>
				<name>BVT</name>
			</listView>-->
			<listView>
				<name>BVT</name>
				<jobNames>
					<string>commerce-BVT</string>
				</jobNames>
			</listView>
		</views>
	</jenkins>
	
	<report>
		<!-- optional: default template is in the Rerun.jar-->
		<ReportTemplatePath></ReportTemplatePath>
	</report>

	<log>
		<!-- optional: default is KeepFailure     (KeepAll,KeepNone,KeepSuccess,KeepFailure)-->
		<keepLog></keepLog>
		<!-- optional: default is 50-->
		<daysToKeep>50</daysToKeep>
		<!-- optional: default is  currentPah+"\\rerunLog"-->
		<logPath></logPath>
	</log>



</rerunConfig>