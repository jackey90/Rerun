<rerunConfig>
	<jenkins>
		<jenkinsFolder>C:\Program Files (x86)\Jenkins</jenkinsFolder>
		<!-- choose the view you want to rerun -->
		<!--
		<views>
			<listView>
				<name>BVT</name>
			</listView>
			<listView>
				<name>Comprehensive</name>
			</listView>
		</views>
		  -->
	</jenkins>

	<report>
		<!-- optional: default template is in the Rerun.jar -->
		<ReportTemplatePath></ReportTemplatePath>
		<!-- optional: default is currentPath+"\\" -->
		<ReportOutPutPath></ReportOutPutPath>
	</report>

	<log>
		<!-- KeepAll,KeepNone,KeepSuccess,KeepFailure -->
		<keepLog></keepLog>
		<!-- optional: default is 50 -->
		<daysToKeep></daysToKeep>
		<logPath></logPath>
	</log>

</rerunConfig>